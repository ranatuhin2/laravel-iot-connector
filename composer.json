{
     "name": "rana-tuhin/laravel-iot-connector",
     "description": "A Laravel package to connect and manage IoT devices.",
     "type": "library",
     "license": "MIT",
     "authors": [
          {
               "name": "Tuhin Rana Mondal",
               "email": "aimlesstuhin@gmail.com"
          }
     ],
     "require": {
          "php": "^8.0",
          "illuminate/support": "^10.0|^11.0|^12.0",
          "illuminate/database": "^10.0|^11.0|^12.0",
          "php-mqtt/client": "^1.0"
     },
     "autoload": {
          "psr-4": {
               "RanaTuhin\\LaravelIoTConnector\\": "src/"
          }
     },
     "extra": {
          "laravel": {
               "providers": [
                    "RanaTuhin\\LaravelIoTConnector\\IoTServiceProvider"
               ]
          },
          "icon": "resources/icon.png"
     },
     "minimum-stability": "dev",
     "prefer-stable": true
}
